#include<iostream>
#include<vector>
using namespace std;

class Solution {
public:
    int minDeletionSize(vector<string>& strs) {
        int count = 0;
        int row = strs.size();
        int col = strs[0].size();
        for (int i = 0; i < col; ++i)                      //循环逐列比较
        {
            for (int j = 0; j < row - 1; ++j)
            {
                if (strs[j][i] > strs[j + 1][i])
                {
                    ++count;
                    break;
                }
            }
        }
        return count;
    }
};

int main()
{
    //测试
    //vector<string> strs = { "cba", "daf", "ghi" };          //返回1
    //Solution solution;
    //cout << solution.minDeletionSize(strs) << endl;

    return 0;
}
